<div id="<%= dom_id company %>">
  <p>
    <strong>Name:</strong>
    <%= company.name %>
  </p>

  <p>
    <strong>Address:</strong>
    <%= company.address %>
  </p>

  <p>
    <strong>Reg no:</strong>
    <%= company.reg_no %>
  </p>

  <p>
    <strong>Phone:</strong>
    <%= company.phone %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= company.email %>
  </p>

  <p>
    <strong>Contact:</strong>
    <%= company.contact %>
  </p>

  <p>
    <strong>Bank:</strong>
    <%= company.bank_id %>
  </p>

  <p>
    <strong>Account number:</strong>
    <%= company.account_number %>
  </p>

</div>
